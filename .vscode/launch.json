{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/server.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect-wait",
        "-A",
        "--unstable-kv"
      ],
      "attachSimplePort": 9229
    }
  ]
}
